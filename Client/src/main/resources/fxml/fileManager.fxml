               <image>
                  <Image url="@../img/download.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button layoutX="748.0" layoutY="574.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="150.0" text="   Delete" textFill="#f80505">
