            <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/upload.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button layoutX="516.0" layoutY="574.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="150.0" text=" Download" textFill="#159711">
