                        <Insets bottom="5.0" right="5.0" top="1.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="butUserDir" mnemonicParsing="false" onAction="#onButtonUserDir" prefHeight="25.0" prefWidth="67.0" text="Set Dir" />
